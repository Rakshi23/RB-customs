<script src="https://code.jquery.com/jquery-1.12.4.min.js"></script>
<script type="text/javascript">
    $(document).ready(function(){
        $(".orderplaced").click(function(){
            $(this).text($(this).text() == 'PLACED' ? 'ALREADY ORDERED' : 'PLACED');
        });
    });

</script>

<script type="text/javascript">
  $(document).ready(function(){
      $(".reject").click(function(){
          $(this).text($(this).text() == 'REJECTED' ? 'REJECTED' : 'REJECTED');
         // $(".approve").addClass("disabled");
      });
  });
</script>

<script type="text/javascript">
  $(document).ready(function(){
      $(".approve").click(function(){
          $(this).text($(this).text() == 'APPROVED' ? ' APPROVED' : 'APPROVED');
         // $(".reject").addClass("disabled");
      });
  });
  $("#ap1").click(function(){
    $("#re1").addClass("disabled");
  })
  $("#re1").click(function(){
    $("#ap1").addClass("disabled");
  })
//******************
  $("#ap2").click(function(){
    $("#re2").addClass("disabled");
  })
  $("#re2").click(function(){
    $("#ap2").addClass("disabled");
  })
//******************
  $("#ap3").click(function(){
    $("#re3").addClass("disabled");
  })
  $("#re3").click(function(){
    $("#ap3").addClass("disabled");
  })
//******************
$("#ap4").click(function(){
    $("#re4").addClass("disabled");
  })
  $("#re4").click(function(){
    $("#ap4").addClass("disabled");
  })
</script>

